@inject IJSRuntime JSRuntime
<nav>
      <div class="container mx-auto px-10">
          <div class="flex justify-between h-16 items-center uppercase">
              <div>NewsApi.BlazorClient</div>
              <div>
                  <button @onclick="ToggleBtn" class="px-3 py-3 border border-gray-200">
                      Switch Theme
                  </button>
              </div>
          </div>
      </div>
</nav>

@code{
    private async Task ToggleBtn()
    {
        await JSRuntime.InvokeVoidAsync("toggleClass");
    }
}